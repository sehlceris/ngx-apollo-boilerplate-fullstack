{
  "name": "ng2-apollo-boilerplate-client",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "npm run lint && ng test --configuration=test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch",
    "build:prod": "ng build --prod --build-optimizer --vendor-chunk --common-chunk",
    "clean": "rimraf ./dist/",
    "server": "cd dist && http-server",
    "prod": "npm run clean && npm run build:prod && npm run server",
    "ci": "npm run clean && npm run prettier:ci && ng lint && ng test --configuration=test --browsers ChromeTravisCi && ng e2e && npm run build:prod -- --deploy-url /angular-ngrx-material-starter/ --base-href /angular-ngrx-material-starter",
    "release": "standard-version && git push --follow-tags origin master",
    "prettier": "prettier '{src,e2e}/**/*.{ts,json,md,scss}' --write",
    "prettier:ci": "prettier '{src,e2e}/**/*.{ts,json,md,scss}' --list-different",
    "analyze": "npm run clean && npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "generate": "npm run generate:types && npm run generate:client:apollo",
    "generate:types": "rm src/app/generated/anms.typings.ts; gql-gen --schema http://localhost:3000/graphql --template graphql-codegen-typescript-template --out src/app/generated/anms.typings.ts 'src/app/graphql/operations/**/*.graphql'",
    "generate:client:apollo": "rm src/app/generated/anms-graphql-client.ts; gql-gen --schema http://localhost:3000/graphql --template graphql-codegen-apollo-angular-template --out src/app/generated/anms-graphql-client.ts 'src/app/graphql/operations/**/*.graphql'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && npm run lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.7",
    "@angular/compiler": "^6.1.7",
    "@angular/core": "^6.1.7",
    "@angular/forms": "^6.1.7",
    "@angular/http": "^6.1.7",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.7",
    "@angular/platform-browser-dynamic": "^6.1.7",
    "@angular/router": "^6.1.7",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "apollo-angular": "^1.3.0",
    "bootstrap": "^4.1.3",
    "browser-detect": "^0.2.28",
    "core-js": "^2.5.7",
    "hammerjs": "^2.0.8",
    "material-icons": "^0.2.3",
    "moment": "^2.22.2",
    "rxjs": "^6.3.2",
    "uuid": "^3.3.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26",
    "apollo-angular-link-http": "^1.2.0",
    "apollo-link": "^1.2.2",
    "apollo-client": "^2.4.2",
    "apollo-cache-inmemory": "^1.2.10",
    "graphql-tag": "^2.9.2",
    "graphql": "^0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.1",
    "@angular/cli": "~6.2.1",
    "@angular/compiler-cli": "^6.1.7",
    "@angular/language-service": "^6.1.7",
    "@types/jasmine": "~2.8.8",
    "@types/node": "~10.9.4",
    "codelyzer": "~4.4.4",
    "graphql-code-generator": "^0.11.0",
    "graphql-codegen-apollo-angular-template": "^0.11.0",
    "graphql-codegen-typescript-template": "^0.11.0",
    "husky": "^1.0.0-rc.13",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "~1.3.1",
    "karma-spec-reporter": "~0.0.32",
    "ngrx-store-freeze": "^0.2.4",
    "prettier": "^1.14.2",
    "protractor": "^5.4.1",
    "rimraf": "^2.6.2",
    "standard-version": "^4.4.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~2.9.0",
    "webpack-bundle-analyzer": "^3.0.2"
  }
}
